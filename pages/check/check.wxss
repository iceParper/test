/* pages/check/check.wxss */
page {
  background-color:#F4F5F4;
}
.top{
  width: 750rpx;
  display: flex;
}
.search-go{
  margin: 20rpx 0rpx 0rpx 20rpx;
  width: 630rpx;
  height: 92rpx;
  background-color: #FFFFFF;
  border-radius: 8rpx;
  text-align: center;
  box-shadow: 0px 2px 6px 0px rgba(0, 0, 0, 0.23);
  display: flex;
}
.search-img{
  margin:22rpx 0rpx 22rpx 24rpx ;
  width: 48rpx;
  height: 48rpx;
}
.input{
  margin: 22rpx auto 0rpx 16rpx;
  background-color: #FFFFFF;
  font-size: 34rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.search-close{
  margin: 24rpx;
}
.gosearch{
  width: 68rpx;
  height: 48rpx;
  padding-top: 42rpx;
  margin-left: 12rpx;
  color: rgba(16, 16, 16, 100);
  font-size: 34rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.coupon{
  width: 702rpx;
  height: 220rpx;
  margin: 24rpx;
  background-color: rgba(255, 255, 255, 100);
  color: rgba(16, 16, 16, 100);
  font-family: Arial;
  border: 1rpx solid rgba(228, 112, 54, 100); 
  display: flex; 
}
.money{
  width: 498rpx;
  border-right-style: dotted;
  border-right-color: #E47036;
}
.money_title{
  height: 40rpx;
  padding: 10rpx;
  line-height: 40rpx;
  margin-left: 30rpx;
  color: rgba(190, 190, 190, 100);
  font-size: 20rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.money_main{
  width: 100%;
  height: 160rpx;
  display: flex;
}
.money_sum{
  font-size: 96rpx;
  color: rgba(228, 112, 54, 100);
  text-align: left;
  line-height: 100rpx;
  padding-left: 44rpx;
  font-family: 方正工业黑-标准;
  font-weight: 900;
}
.money_type{
  color: rgba(190, 190, 190, 100);
  font-size: 20rpx;
  line-height: 20rpx;
  text-align: left;
  margin-top: 40rpx;
  margin-left: 12rpx;
  font-family: PingFangSC-regular;
}
.money_condition{
  color: rgba(190, 190, 190, 100);
  font-size: 28rpx;
  line-height: 30rpx;
  margin-top: 12rpx;
  margin-left: 12rpx;
  font-family: PingFangSC-regular;
}
.gobuy{
  width: 202rpx !important;
  height: 220rpx;
  background-color: #E47036;
  text-align: center;
}
.gobuy_goto{
  margin-top: 10rpx;
  font-weight: 600;
  color: #FFFFFF;
  font-size: 24rpx;
  line-height: 220rpx;
  font-family: PingFangSC-bold;
}
.bottom_used{
